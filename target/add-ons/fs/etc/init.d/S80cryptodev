#!/bin/sh
#
# Cryptodev.
#
# Written by Vladik Goytin.

cryptodev_start()
{
	insmod /lib/modules/cryptodev.ko
}


case "$1" in
start)
	logger "S80"
	sync
	cryptodev_start
	logger "S80 done"
	sync
	;;
stop)
	;;
restart|reload)
	"$0" stop
	"$0" start
	;;
*)
	echo "Usage: $0 {start|stop|restart}"
	exit 1
esac

exit $?

